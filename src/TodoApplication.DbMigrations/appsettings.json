{
  "ConnectionStrings": {
    "Database": "Host=host.docker.internal;Port=5432;Database=product;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://todoapplication.seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "DbSetting": {
    "Timeout": "600",
    "MigrationTableName": "__EFMigrationHistory",
    "ConnectionString": "Host=host.docker.internal;Port=5432;Database=product;Username=postgres;Password=postgres;Include Error Detail=true"
  }
}
